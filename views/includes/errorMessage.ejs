<% var msg=getMessages() %>

  <% if (msg.error) { %>
    <div class="alert alert-danger" role="alert">
      <% msg.error.forEach(msg=> { %>
        <div>
          <%= msg %>
        </div>
        <% })%>
    </div>
    <% } %>

      <% if(errorMessage) { %>
        <div class="alert alert-danger" role="alert">
          <%= errorMessage %>
        </div>
        <% } %>